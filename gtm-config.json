                "name": "Get Filter Name",
                "parameter": [
                    {
                        "key": "javascript",
                        "type": "template",
                        "value": "function() {\n\tvar parent = {{Find Closest}}({{Click Element}}, '.js_FilterOption')\n    var filterLabel = parent.getElementsByTagName('span')[0]\n    if (filterLabel) {\n\t\treturn filterLabel.innerText.toLowerCase()      \n    }\n\treturn ''\n}"
                    }
                ],
                "type": "jsm",
                "variableId": "6"
            },
            {
                "accountId": "4134047533",
                "containerId": "10046994",
                "name": "Find Closest",
                "parameter": [
                    {
                        "key": "javascript",
                        "type": "template",
                        "value": "function() {\n  return function(target, selector) {\n    while (!target.matches(selector) \u0026\u0026 !target.matches('body')) {\n      target = target.parentElement;\n    }\n    return target.matches(selector) ? target : undefined;\n  }\n}"
                    }
                ],
                "type": "jsm",
                "variableId": "7"
            },
